#!/usr/bin/env node

var analyze = require('../analyze'),
    pipeline = require('../pipeline');

var name  = process.argv[2] || 'module-smith',
    mod   = process.argv[3] || 'async',
    files = require('../test/' + name + '.json');

try {
  var results = analyze(files, mod);
  console.dir(results);
}
catch (ex) {
  console.dir(ex);
  if (ex.stack) {
    console.log(ex.stack);
  }
}
